def najwieksza_srednia(lista1, lista2, lista3):
    srednia1 = sum(lista1) / len(lista1)
    srednia2 = sum(lista2) / len(lista2)
    srednia3 = sum(lista3) / len(lista3)

    if srednia1 > srednia2 and srednia1 > srednia3:
        return "Największa średnia znajduje się w liście 1."
    elif srednia2 > srednia1 and srednia2 > srednia3:
        return "Największa średnia znajduje się w liście 2."
    else:
        return "Największa średnia znajduje się w liście 3."



